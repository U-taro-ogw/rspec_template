# Ignore bundler config.
rails_api/.bundle

# Ignore all logfiles and tempfiles.
rails_api/log/*
rails_api/tmp/*
!rails_api/log/.keep
!rails_api/tmp/.keep


# Ignore uploaded files in development
rails_api/storage/*
!rails_api/storage/.keep

.byebug_history

# Ignore master key for decrypting credentials and more.
rails_api/config/master.key

